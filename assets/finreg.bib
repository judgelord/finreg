@book{Wilson1989,
	abstract = {Wilson examines bureaucratic efficiency and capacity, and attempts to explain the agendas bureaucracies set out to realize and the constraints that hamper their ability to execute effectively. He outlines three primary constraints (Chapter 7) on bur eaucratic behavior: (1) they lack a profit maximizing incentive, since they are legally precluded from distributing profits to their clients; (2) they are unable to dynamically allocate resources to areas of greatest need or even greatest success; and fina lly (3) bureaucratic agencies serve goals that are not of their own choosing. Importantly, many of these constraints are not the result of internal failure but are imposed from external actors like Congress. This ``red tape'' is motivated by a desire to prevent agency misbehavior. Wilson concludes that these constraints lead to overly complex procedures and institutional rules, an incentive structure that aligns more closely with minimizing constraint violation than effective outcomes, and a proliferation of middle management who are incentivized to be risk averse and shy from innovation. As such, the inefficiency many experience in their interactions with bureaucracy isn't necessarily a result of agencies eager to assume an ever-increasing number of functions ; rather it is often their need to adhere to specific policy prescriptions that leads to inefficiency. Bureaucracy is the classic study of the way American government agencies work and how they can be made to work better. Examining a wide range of bureaucracies, including the Army, the FBI, the FCC, and the Social Security Administration, James Q. Wilson provides the first comprehensive, in-depth analysis of what government agencies do, why they function as they do, and how they might become more responsible and effective. With a new introduction by the author. Armies, prisons, schools -- Organization matters -- Circumstances -- Beliefs -- Interests -- Culture -- Constraints -- People -- Compliance -- Turf -- Strategies -- Innovation -- Congress -- Presidents -- Courts -- National differences -- Problems -- Rules -- Markets -- Bureaucracy and the public interest.},
	author = {Wilson, James Q.},
	isbn = {0465007856},
	pages = {433},
	publisher = {Basic Books},
	title = {{Bureaucracy: what government agencies do and why they do it}},
	year = {1989}}

