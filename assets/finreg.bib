@article{Volden2002,
	abstract = {Political economists have modeled attempts by politicians to form$\backslash$nand empower bureaucracies. Some of these models demonstrate that$\backslash$ndelegation is greater under unified government. Scholars might expect$\backslash$nthat discretion would thus decline under divided government. However,$\backslash$nlegislators who attempt to limit executive agency discretion under$\backslash$ndivided government face the threat or use of an executive veto. This$\backslash$narticle expands previous formal models of discretion to include status$\backslash$nquo policies and levels of discretion, various agency preferences,$\backslash$nand the possibility of an executive veto, in order to determine the$\backslash$nlevel of bureaucratic discretion chosen in a separation of powers$\backslash$nsystem. Three main findings emerge: (1) given certain status quo$\backslash$nconditions and preference arrangements, discretion can be increased$\backslash$nor decreased under either divided or unified government; (2) executive$\backslash$nagencies are more likely to receive discretion under unified government,$\backslash$nbut independent agencies are more likely to receive discretion under$\backslash$ndivided government; and (3) there exists an asymmetry between politicians'$\backslash$nability to expand bureaucratic discretion and their ability to restrict$\backslash$ndiscretion.},
	author = {Volden, Craig},
	date-added = {2021-09-27 14:50:01 -0400},
	date-modified = {2021-09-27 14:50:01 -0400},
